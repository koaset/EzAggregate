{
	"database": {
		"url":  "mongodb://localhost:27017/",
		"name": "test"
	},
	"restapi" : {
		"source_port": "8079",
		"output_port": "8080"
	},
	"stores": [ 
		"banana_store"
	],
	"sources": [
		{
			"name": "banana_source",
			"type": "restapi",
			"store": "banana_store",
			"path": "/bananas",
			"fields": {
				"user_id": "string",
				"name": "string",
				"num_bananas": "number"
			}
		}
	],
	"outputs": [
		{
			"name": "bananapi",
			"type": "restapi",
			"path": "/bananas",
			"store": "banana_store",
			"aggregation": {
				"name": "sum_bananas",
				"key": "user_id",
				"field": "num_bananas",
				"action": "sum",
				"order_by": "sum_bananas",
				"invert": true
			},
			"timeframe": "1h"
		}
	]
}
