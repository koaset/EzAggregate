{
	"database":{
		"url": "mongodb://localhost:27017/",
		"name": "test"
	},
	"restapi":{
		"source_port": "8079",
		"output_port": "8080"
	},
	"stores":[
		{
			"name":"banana_store",
			"sources":[
				"source1"
			],
			"key":"user_id",
			"fields":[
				"name",
				"num_bananas"
			]
			
		}
	],
	"sources":[
		{
			"name":"source1",
			"type":"restapi",
			"store":"banana_store",
			"path":"/bananas",
			"fields":{
				"user_id":"string",
				"name":"string",
				"num_bananas":"number"
			}
		}
	],
	"outputs":[
		{
			"name":"bananapi",
			"type":"restapi",
			"port":"8080",
			"path":"/bananas",
			"store_name":"banana_store",

			"aggregate_field":"num_bananas",
			"aggregate_action":"sum",

			"query_fields":[ 
				"user_id" 
			],
			
			"timeframe":"1h"
		}
	]
}
